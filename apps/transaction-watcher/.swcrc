{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": "inline",
  "inputSourceMap": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "useDefineForClassFields": false
    },
    "keepClassNames": true
  },
  "exclude": [],
  "minify": false
}
